# service error codes

declare -A ERR_CODE

SRV="link"
eval "ERR_DESC_$SRV[1]='ping fail'"
eval for idx in \"\${!ERR_DESC_${SRV}[@]}\" \; do ERR_CODE[${SRV},\$idx]=\"\${ERR_DESC_${SRV}[\$idx]}\" \; done

